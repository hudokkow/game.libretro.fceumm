<?xml version="1.0" encoding="UTF-8"?>
<addon id="game.libretro.fceumm"
		name="NES (FCEUmm)"
		version="0.98.13"
		provider-name="Libretro">
	<requires>
		<import addon="game.libretro" version="1.0.0"/>
		<import addon="game.controller.nes" version="1.0.0"/>
	</requires>
	<extension point="kodi.gameclient"
                   library_android="libgame.libretro.fceumm.so"
                   library_linux="game.libretro.fceumm.so"
                   library_osx="game.libretro.fceumm.dylib"
                   library_windx="game.libretro.fceumm.dll"
                   library_wingl="game.libretro.fceumm.dll">
		<platforms>NES</platforms>
		<extensions>fds|nes|unif</extensions>
		<supports_vfs>false</supports_vfs>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary lang="en">FCEUmm v0.98.13mm</summary>
		<description>Port of FCEUmm to Libretro. FCEUmm emulates NES, Famicom, and Famicom Disk System (FDS). FCEUmm merges elements from FCEU Ultra, FCEU rerecording, FCEUXD, FCEUXDSP, FCEUXDSP CE, and FCEU-mm into a single branch of FCEU.

Supported files: .fds, .nes, .unif

License: GPLv2

Icon credit: Anarkhya (anarkhya.deviantart.com)</description>
		<platform>all</platform>
		<nofanart>true</nofanart>
	</extension>
</addon>
